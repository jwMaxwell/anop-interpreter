;; Joshua Maxwell
;; This test program performs FizzBuzz
(
  (var fizzbuzz (expr (x) 
  (
    if (% x 5)
      (if (% x 3)
        x
        "Fizz")
      (if (% x 3)
        "Buzz"
        "FizzBuzz")
  )))
 
  (var loop (expr (x) 
  (   
    (print (head (fizzbuzz (x)))) ;; head removes outer array
    (var y (+ x 1)) ;; easier to work with
    (if (< y 100) 
      (loop (y)) 
      () ;; good practice
    )
  )))

  (loop (1))
)
